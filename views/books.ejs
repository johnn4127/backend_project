<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <link rel="stylesheet" href="books.css">
</head>
<body>
    <% allBooks.forEach(book => { %>
        <div class="flip-card">
            <div class="flip-card-inner">
                <div class="flip-card-front">
                    <img src = '<%= book.dataValues.images %>'  alt="Avatar" style="width:300px;height:300px;">
                </div>
                <div class="flip-card-back">
                    <h1><%= book.dataValues.book_name %></h1>
                    <p><%= book.dataValues.author %></p>
                    <p><%= book.dataValues.genre %></p>
                </div>
            </div>
        </div>
    <% }); %>
    <img src="https://tinyurl.com/2s3sya9n" alt="hello">
</body>
</html>